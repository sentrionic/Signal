{
  "name": "signal",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit --skipLibCheck",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "npx prettier --write **/*.{ts,js,css,html,tsx,vue}",
    "generate:api": "sta -p ../server/swagger-spec.json -o ./src/lib/api -n index.ts --no-client",
    "workflow": "pnpm format && pnpm lint && pnpm type-check"
  },
  "dependencies": {
    "@heroicons/vue": "^2.0.12",
    "@vueuse/core": "^9.3.0",
    "dayjs": "^1.11.5",
    "flowbite": "^1.5.3",
    "ky": "^0.31.3",
    "pinia": "^2.0.23",
    "socket.io-client": "^4.5.2",
    "vee-validate": "^4.7.0",
    "vue": "^3.2.40",
    "vue-query": "^1.26.0",
    "vue-router": "^4.1.5",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "^18.8.4",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.25.0",
    "eslint-plugin-vue": "^9.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.17",
    "prettier": "^2.7.1",
    "swagger-typescript-api": "^10.0.1",
    "tailwindcss": "^3.1.8",
    "typescript": "~4.8.4",
    "vite": "^3.1.7",
    "vue-tsc": "^1.0.3"
  }
}
